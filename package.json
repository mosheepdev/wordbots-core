{
  "name": "wordbots",
  "version": "0.10.0-alpha",
  "description": "A card-and-board game that showcases card game semantic parsing",
  "license": "MIT",
  "browserslist": [
    "last 4 versions",
    "chrome >= 48"
  ],
  "scripts": {
    "analyze": "npm run prebuild && yarn run webpack -p --config webpack.build.config.js --progress --devtool source-map --display minimal && source-map-explorer dist/bundle.js",
    "build": "npm run clean && npm run prebuild && set NODE_ENV=production && webpack -p --config webpack.build.config.js --progress --colors --profile",
    "build:stats": "npm run prebuild && node_modules/.bin/webpack -p --config webpack.build.config.js --json > stats.json",
    "clean": "node bin/clean.js dist",
    "heroku-postbuild": "npm run clean && set NODE_ENV=production && webpack -p --progress --colors --profile",
    "lint": "node_modules/eslint/bin/eslint.js src test && node_modules/tslint/bin/tslint src/**/*.ts",
    "prebuild": "sed 's/export default webpackConfig;/module.exports = webpackConfig;/' <webpack.config.js >webpack.build.config.js",
    "slate": "node bin/clean.js node_modules",
    "start": "set NODE_ENV=development && ts-node src/server/index.ts --colors --profile",
    "start-prod": "set NODE_ENV=production && ts-node src/server/index.ts --progress --colors --profile",
    "test": "jest",
    "test:watch": "npm test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wordbots/wordbots-core.git"
  },
  "dependencies": {
    "@babel/core": "^7.0.1",
    "@material-ui/core": "^3.0.1",
    "awesome-typescript-loader": "^5.0.0",
    "babel-loader": "^8.0.1",
    "better-react-spinkit": "^2.0.4",
    "cookie-parser": "^1.4.3",
    "copy-webpack-plugin": "^4.5.0",
    "css-loader": "^1.0.0",
    "express": "^4.16.1",
    "fast-json-patch": "^2.0.5",
    "file-loader": "^1.1.11",
    "firebase": "^5.0.2",
    "image-webpack-loader": "^4.0.0",
    "json-loader": "^0.5.7",
    "jsx-loader": "^0.13.2",
    "lodash": "^4.17.5",
    "material-ui": "^0.20.0",
    "merge": "1.2.0",
    "moment": "^2.22.2",
    "prop-types": "^15.6.1",
    "rc-slider": "^8.6.1",
    "react": "^16.5.2",
    "react-addons-perf": "^15.4.2",
    "react-bar-chart": "^0.1.2",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dom": "^16.5.2",
    "react-ga": "^2.5.3",
    "react-helmet": "^5.2.0",
    "react-hot-loader": "^4.3.3",
    "react-iframe": "^1.1.0",
    "react-markdown": "^4.0.0",
    "react-popover": "^0.5.4",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-slick": "^0.23.0",
    "react-sound": "1.2.0",
    "react-tap-event-plugin": "^3.0.3",
    "react-textfit": "^1.0.1",
    "react-tooltip": "^3.3.0",
    "react-transition-group": "^2.2.1",
    "react-web-notification": "^0.3.1",
    "redux": "^4.0.0",
    "redux-devtools": "^3.4.0",
    "redux-devtools-dock-monitor": "^1.1.3",
    "redux-devtools-log-monitor": "^1.3.0",
    "redux-logger": "3.0.6",
    "repng": "^1.0.1",
    "screenfull": "^3.3.1",
    "seed-random": "^2.2.0",
    "shuffle-array": "^1.0.1",
    "soundmanager2": "^2.97.20170602",
    "source-map-loader": "^0.2.3",
    "style-loader": "0.23.0",
    "substitute-loader": "^1.0.0",
    "trianglify": "^1.1.0",
    "ts-node": "^7.0.1",
    "url-loader": "^1.0.1",
    "webpack": "^4.0.0",
    "webpack-cli": "^3.0.8",
    "whatwg-fetch": "^2.0.3",
    "word-ngrams": "^0.2.0",
    "ws": "^6.0.0",
    "yarn": "^1.7.0"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.1",
    "@types/express": "^4.16.0",
    "@types/history": "^4.6.2",
    "@types/jest": "^23.1.0",
    "@types/lodash": "^4.14.109",
    "@types/seed-random": "^2.2.0",
    "@types/webpack": "^4.0.0",
    "@types/ws": "^6.0.0",
    "babel-eslint": "^10.0.1",
    "csstype": "^2.5.2",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^5.0.0",
    "eslint-plugin-compat": "^2.6.2",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jest": "^21.12.3",
    "eslint-plugin-lodash": "^3.1.0",
    "eslint-plugin-no-loops": "^0.3.0",
    "eslint-plugin-promise": "^4.0.0",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-unicorn": "^6.0.0",
    "expect": "^23.0.1",
    "ignore-styles": "^5.0.1",
    "indefinite-observable": "^1.0.1",
    "jest": "^22.4.2",
    "jsdom-no-contextify": "3.1.0",
    "mocha": "^5.0.1",
    "mock-socket": "^8.0.2",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "^16.5.2",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.4",
    "redbox-react": "^1.5.0",
    "ts-jest": "^23.10.2",
    "ts-loader": "^4.3.0",
    "tslint": "^5.10.0",
    "typescript": "^2.8.3",
    "webpack-bundle-analyzer": "^3.0.2",
    "webpack-dev-middleware": "2.0.4",
    "webpack-dev-server": "^3.1.0",
    "webpack-hot-middleware": "^2.21.2"
  },
  "optionalDependencies": {
    "canvas-prebuilt": "^1.6.0"
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test",
      "src/common/components/hexgrid/",
      "src/common/components/Sprite.js"
    ],
    "globals": {
      "_": {}
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "setupFiles": [
      "./test/beforeTest.js"
    ],
    "testRegex": "(\\.|/)(test|spec)\\.(jsx?|tsx?)$",
    "transform": {
      "^.+\\.(j|t)sx?$": "ts-jest"
    }
  },
  "greenkeeper": {
    "ignore": [
      "webpack",
      "webpack-dev-middleware"
    ]
  }
}
